<Window x:Class="Highscores.HighscoreWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Highscores"
        mc:Ignorable="d"
        Title="HighscoreWindow" Height="450" Width="400" Loaded="Window_Loaded" Closed="Window_Closed">
    <Grid>
        <StackPanel x:Name="HighscoreStackPanel" HorizontalAlignment="Center" />
    </Grid>
</Window>
